			var ancho = Math.max(document.documentElement.clientWidth, window.innerWidth || document.body.ClientWidth || 0);
			drawSlider($("#trades-slider"),result["vids"].length,"trades",3)
			renderSlider("trades",$("#trades-container"),$("#trades-cont"),$("#trades-vids"),result["vids"],"trades-vid-cont","trades-slide-vid",(ancho < 792 ? 2 : 3),(ancho < 792 ? 0.4 : 0.3),false,"trades");
			drawSlider($("#migdep-slider"),migdep_links,"migdep",3)
			renderSlider("migdep",$("#migdep-container"),$("#migdep-cont"),$("#migdep-vids"),migdep_links,"migdep-vid-cont","migdep-slide-vid",(ancho < 792 ? 2 : 3),(ancho < 792 ? 0.4 : 0.3),false,"migdep");
			drawSlider($("#sedes-slider"),sedes_links,"sedes",3)
			renderSlider("sedes",$("#sedes-container"),$("#sedes-cont"),$("#sedes-vids"),sedes_links,"sedes-vid-cont","sedes-slide-vid",(ancho < 792 ? 2 : 3),(ancho < 792 ? 0.4 : 0.3),false,"sedes");
		}
	});

$(window).on("resize",function(){
			var ancho = Math.max(document.documentElement.clientWidth, window.innerWidth || document.body.ClientWidth || 0);
			renderSlider("trades",$("#trades-container"),$("#trades-cont"),$("#trades-vids"),trades_links,"trades-vid-cont","trades-slide-vid",(ancho >= 792 ? 3 : (ancho >= 545 ? 2 : 1)),(ancho >= 792 ? 0.3 : (ancho >= 545 ? 0.4 : 0.47)),true,"trades");
			renderSlider("migdep",$("#migdep-container"),$("#migdep-cont"),$("#migdep-vids"),migdep_links,"migdep-vid-cont","migdep-slide-vid",(ancho >= 792 ? 3 : (ancho >= 545 ? 2 : 1)),(ancho >= 792 ? 0.3 : (ancho >= 545 ? 0.4 : 0.47)),true,"migdep");
			renderSlider("sedes",$("#sedes-container"),$("#sedes-cont"),$("#sedes-vids"),sedes_links,"sedes-vid-cont","sedes-slide-vid",(ancho >= 792 ? 3 : (ancho >= 545 ? 2 : 1)),(ancho >= 792 ? 0.3 : (ancho >= 545 ? 0.4 : 0.47)),true,"sedes");
});
